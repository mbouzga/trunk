<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	id="WebApp_ID" version="3.0">
	<display-name>petshop</display-name>
	<welcome-file-list>
		<welcome-file>categories.jsp</welcome-file>
	</welcome-file-list>

	<servlet>
		<servlet-name>accessoiresControle</servlet-name>
		<servlet-class>controlleurs.accessoiresControle</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>accessoiresControle</servlet-name>
		<url-pattern>/access</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>holidaysgiftsControle</servlet-name>
		<servlet-class>controlleurs.holidaysgiftsControle</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>holidaysgiftsControle</servlet-name>
		<url-pattern>/*</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>petsgiftsControle</servlet-name>
		<servlet-class>controlleurs.petsgiftsControle</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>petsgiftsControle</servlet-name>
		<url-pattern>/*</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>specialsControle</servlet-name>
		<servlet-class>controlleurs.specialsControle</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>specialsControle</servlet-name>
		<url-pattern>/*</url-pattern>
	</servlet-mapping>

</web-app>